<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <TableView fx:id="transactionTableView" minWidth="500.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <placeholder>
            <Label text="Nothing" />
        </placeholder>
        <columns>
            <TableColumn fx:id="indexTableColumn" editable="false" maxWidth="2000.0" minWidth="-1.0" prefWidth="-1.0" text="No." />
            <TableColumn fx:id="descriptionTableColumn" editable="false" maxWidth="8000.0" minWidth="-1.0" prefWidth="75.0" text="Description" />
            <TableColumn fx:id="amountTableColumn" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="75.0" text="Amount" />
            <TableColumn fx:id="dateTableColumn" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="75.0" text="Date" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</VBox>
