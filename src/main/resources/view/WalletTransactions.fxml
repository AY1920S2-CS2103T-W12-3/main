<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="transactionTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="500.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <placeholder>
            <Label text="Nothing" />
         </placeholder>
         <columns>
            <TableColumn fx:id="indexTableColumn" editable="false" maxWidth="3000.0" minWidth="-1.0" prefWidth="-1.0" text="No." />
            <TableColumn fx:id="descriptionTableColumn" editable="false" maxWidth="7000.0" minWidth="-1.0" prefWidth="75.0" text="Description" />
            <TableColumn fx:id="amountTableColumn" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="75.0" text="Amount" />
            <TableColumn fx:id="dateTableColumn" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="75.0" text="Date" />
            <TableColumn fx:id="tagTableColumn" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="75.0" text="Tag" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
